{
  "org": "$HZN_ORG_ID",
  "label": "$SERVICE_NAME for $ARCH",
  "description": "A network monitoring service",
  "public": true,
  "documentation": "",
  "url": "$SERVICE_NAME",
  "version": "$SERVICE_VERSION",
  "arch": "$ARCH",
  "sharable": "multiple",
  "requiredServices": [
    {
      "url": "couchdbdual",
      "org": "Maxwell.McAdam@ibm.com",
      "version": "0.0.1",
      "arch": "amd64"
    }
  ],
  "userInput": [
      {
        "name": "TEST_VARIABLE",
        "label": "Are variables required",
        "type": "string",
        "defaultValue": ""
      }
  ],
  "deployment": {
      "services": {
          "$SERVICE_NAME": {
              "image": "maxwellmcadam/${DOCKER_IMAGE_BASE}_$ARCH:$SERVICE_VERSION"
          }
      }
  }
}
